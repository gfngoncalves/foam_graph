FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    valueBoundary
    {
        type            fixedValue;
        value           uniform 1;
    }
    zeroGradBoundary
    {
        type            zeroGradient;
    }
    emptyBoundary
    {
        type            empty;
    }
}

